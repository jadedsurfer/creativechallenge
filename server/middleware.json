{
  "initial:before": {
    "serve-favicon": {
      "params": "$!../client/favicon.ico"
    }
  },
  "initial": {
    "morgan": {
      "params": ["dev",{}]
    },
    "compression": {
      "enabled": true,
      "params": {
        "threshold": 512
      }
    },
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {
  },
  "auth": {
  },
  "parse": {
    "body-parser#json": {},
    "body-parser#urlencoded": {
      "params": {
        "extended": true
      }
    }
  },
  "routes": {
    "loopback#status": {
      "paths": "/status"
    }
  },
  "files": {
    "loopback#static": {
      "params": "$!../client"
    }
  },
  "final": {
    "./middleware/custom-url-not-found": {}
  },
  "final:after": {
    "errorhandler": {}
  }
}
